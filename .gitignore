# Ignore directories generated by Composer
drush/contrib
vendor
bin
web/*

# Include the custom themes
# Un-ignore parent trails and then re-ignore
# each parent to allow traversal of directories 
!web/themes
web/themes/*
!web/themes/custom

# Include the custom modules
# Un-ignore parent trails and then re-ignore
# each parent to allow traversal of directories 
!web/modules
web/modules/*
!web/modules/custom

# Ignore Drupal's private file directory
private

# Manage which files to include in web/sites/*
# Un-ignore parent trails and then re-ignore
# each parent to allow traversal of directories
!web/sites
web/sites/*
!web/sites/development.services.yml
!web/sites/*/
web/sites/*/*
!web/sites/*/services.yml
!web/sites/*/settings.php
!web/sites/*/example*
!web/sites/*/files
web/sites/*/files/*
!web/sites/*/files/config*

# Include the libraries directory
!web/libraries

# Include .htaccess and robots.txt files
!web/.htaccess
!web/robots.txt

# Ignore files generated by PhpStorm
.idea
.idea/*
.idea/.*

# Ignore database dumps.
db/*

# Logs and databases
*.log
*.sql
*.sql.gz
*.sqlite

# OS generated files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Packages
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.tar.gz
*.zip

# Compiled source
*.com
*.class
*.dll
*.exe
*.o
*.so

# Ignore automatically generated temporary files
*.~lock*
*~

# Ignore merge tool backups
*.orig

# Ignore node modules
node_modules
